{
  "name": "db-document-cronjob-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all db:start start:local:app",
    "start:local:app": "nodemon app.js",
    "db:start": "docker-compose up -d --build",
    "db:migrate:undo:name": "sequelize-cli db:migrate:undo --name $NAME --env local --config ./config/database.config.js",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --env local --config ./config/database.config.js",
    "db:migrate": "sequelize-cli db:migrate --env local --config ./config/database.config.js",
    "db:delete": "sequelize-cli db:migrate:undo:all --env local --config ./config/database.config.js",
    "migration:create": "sequelize-cli migration:generate --name",
    "refreshModel": "sequelize-cli db:migrate:undo:all --env local --config ./config/database.config.js && sequelize-cli db:migrate --env local --config ./config/database.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyenhoaiphuoc1995/db-document-cronjob-service.git"
  },
  "author": "nguyenhoaiphuoc1995",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nguyenhoaiphuoc1995/db-document-cronjob-service/issues"
  },
  "homepage": "https://github.com/nguyenhoaiphuoc1995/db-document-cronjob-service#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "xlsx": "^0.15.6"
  }
}
